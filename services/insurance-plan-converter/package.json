{
  "name": "insurance-plan-converter",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm -r --filter \"*/packages/backend\" --filter \"*/packages/frontend\" dev",
    "dev:backend": "cd packages/backend && pnpm dev",
    "start:backend": "cd packages/backend && pnpm start",
    "dev:frontend": "cd packages/frontend && pnpm dev",
    "build:frontend": "cd packages/frontend && pnpm build"
  },
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=7.0.0"
  },
  "packageManager": "pnpm@8.15.4"
}
